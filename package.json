  "bugs": {
    "url": "https://github.com/marsczen/mp-deploy-system/issues"
  },
  "homepage": "https://github.com/marsczen/mp-deploy-system#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "husky": "^8.0.1"
  }
}
